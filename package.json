{
  "name": "12capm",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/approuter": "^10.2.0",
    "@sap/cds": "^4",
    "@sap/cds-odata-v2-adapter-proxy": "^1.6.1",
    "@sap/hana-client": "^2.6.61",
    "@sap/xsenv": "^3.1.0",
    "@sap/xssec": "^3.2.0",
    "express": "^4",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.84.7",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run"
  },
  "cds": {
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[production]": {
          "strategy": "JWT"
        },
        "[development]": {
          "strategy": "mock",
          "users": {
            "anubhav": {
              "roles": [
                "Admin",
                "Viewer"
              ],
              "userAttributes": {
                "BankName": [
                  "$UNRESTRICTED"
                ]
              }
            },
            "ananya": {
              "roles": [
                "Viewer"
              ],
              "userAttributes": {
                "BankName": [
                  "My Bank of London",
                  "My Bank of New York"
                ]
              }
            }
          }
        }
      },
      "db": {
        "kind": "hana"
      }
    }
  },
  "sapux": [
    "app/purchaseorderapp"
  ]
}
